{
    "id": "a8-redis",
    "container": {
	"type": "DOCKER",
	"docker": {
	    "image": "redis:alpine",
	    "network": "BRIDGE",
	    "portMappings": [
		{
		    "containerPort": 6379,
		    "hostPort": 31400,
		    "protocol" : "tcp"
		}
	    ]
	}
    },
    "instances": 1,
    "cpus": 0.1,
    "mem": 128
}
